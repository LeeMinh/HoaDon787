@model List<AirlineInvoice.Models.Agent>
<table class="table table-striped table-bordered table-hover" id="tblPq">
    <thead>
        <tr>
            <th title="Tên đại lý">Tên đại lý</th>
            <th title="Tên công ty">Tên công ty</th>
            <th>Mã số thuế</th>
            <th>Mã vùng</th>
            <th>Trạng thái</th>
            <th>Sửa</th>
            <th>Xóa</th>
        </tr>
    </thead>
    <tbody id="tblAgent">
        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
            <tr>
                <td>@item.AgentName</td>
                <td>@item.AgentCompanyName</td>
                <td>
                    @item.AgentTaxCode
                </td>
                <td align="center">@item.SuffixSabre</td>
                <td align="center">
                    @Html.ActionLink(" ", "EditAgent", new { id = AirlineInvoice.Utils.CommonFunction.Shrink(item.AgentID) }, new { @class = "btnAction btnEdit" })
                </td>
                <td align="center"><a href="javascript:void(0);" onclick="confirmDelete('@AirlineInvoice.Utils.CommonFunction.Shrink(item.AgentID)')" class = "btnAction btnDelete">&nbsp;</a> </td>
            </tr>
            }
        }
        else
        {
            <tr>
                <td colspan="6">Không có dữ liệu</td>
            </tr>
        }
    </tbody>
</table>
@Html.Partial("_NavigatePaging")